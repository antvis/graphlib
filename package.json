{
  "name": "@antv/graphlib",
  "version": "2.0.0",
  "scripts": {
    "clean": "rimraf lib esm dist",
    "lint-staged": "lint-staged",
    "lint": "eslint ./src ./__tests__ && prettier ./src ./__tests__ --check ",
    "fix": "eslint ./src ./__tests__ --fix && prettier ./src ./__tests__ --write ",
    "test": "jest --config jest.config.js --coverage -i",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.22",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/antvis/graphlib",
  "bugs": {
    "url": "https://github.com/antvis/graphlib/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/graphlib.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
